<div class="container">
<%= form_for [:admin,@request] do |form| %>
  <div class="form-group">
    <%= form.label :issue_date %><br>
    <%= form.date_select :issue_date %>
  </div>
  <% @request.errors.full_messages_for(:issue_date).each do |message| %>
    <div class="text-danger"><%= message %></div>
  <% end %>

  <div class="form-group">
    <%= form.label :issue_date %><br>
    <%= form.date_select :issue_date %>
  </div>
  </div>
  <% @requesy.errors.full_messages_for(:return_date).each do |message| %>
    <div class="text-danger"><%= message %></div>
  <% end %>

  <div class="form-group">
    <%= form.select :book_id,@books.map{ |c| [c.title,c.id]},{},{:class=>'form-control'}%>
  </div>

   <div>
    <%= form.submit class:'btn btn-outline-secondary' %>
  </div>
<% end %>
</div>